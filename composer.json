{
    "name": "softspring/events",
    "description": "This component provides some useful event classes to use with Symfony",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/form": "^5.4 || ^6.0",
        "symfony/http-foundation": "^5.4 || ^6.0",
        "friendsofphp/php-cs-fixer": "9999999-dev"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\Component\\Events\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\Component\\Events\\Tests\\": "tests/"
        }
    }
}
