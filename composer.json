{
    "name": "softspring/events",
    "description": "This component provides some useful event classes to use with Symfony",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": false,
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^9.5",
        "symfony/form": "^5.4|^6.0|^7.0@dev",
        "symfony/http-foundation": "^5.4|^6.0|^7.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\Component\\Events\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\Component\\Events\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-5.0": "5.0.x-dev",
            "dev-5.1": "5.1.x-dev",
            "dev-5.2": "5.2.x-dev"
        }
    }
}
