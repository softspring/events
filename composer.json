{
    "name": "softspring/events",
    "description": "This component provides some useful event classes to use with Symfony",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "3.48.*",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.5 || ^10.0",
        "symfony/form": "^5.4|^6.0|^7.0",
        "symfony/http-foundation": "^5.4|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\Component\\Events\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\Component\\Events\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
